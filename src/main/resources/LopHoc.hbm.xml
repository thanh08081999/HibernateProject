<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="pojo.LopHoc" table="lop_hoc">
        <id name="tenLop" type="string">
            <column name="tenLop"/>
            <generator class="assigned"/>
        </id>
        <property name="tongSinhVien" type="int">
            <column name="tongSinhVien"/>
        </property>
        <property name="tongNam" type="int">
            <column name="tongNam"/>
        </property>
        <property name="tongNu" type="int">
            <column name="tongNu"/>
        </property>
    </class>
</hibernate-mapping>